#include "stdio.h"
int main()
{
    int n,m;
    int a[100],b[100];
    scanf("%d %d",&n,&m);
    for(int i=0;i<m*n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<m*n;i++)
    {
        scanf("%d",&b[i]);
        if(a[i]!=b[i])
        {
            printf("No\n");
            return 0;
        } 
    }
    printf("Yes\n");
    
}
