#include "stdio.h"
int main()
{
    int a[10],positive=0,negative=0;
    for(int i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
        if(a[i]>0)
            positive++;
        if(a[i]<0)
            negative++;
    }
    printf("positive:%d\n",positive);
    printf("negative:%d\n",negative);
    return 0;
}
